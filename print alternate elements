class GfG
{
    public static void print(int arr[], int n)
    {
        if(n%2==0)
        {
            for(int i=0;i<n-1;i++)
            {
                System.out.print(arr[i]+" ");
                i++;
            }
        }
        else
        {
           for(int i=0;i<n;i++)
            {
                System.out.print(arr[i]+" ");
                i++;
            }  
        }
    }
}
